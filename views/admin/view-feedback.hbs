
<section>
    <div class="container">
    <br><br>
        <table class="table mt-5" id="table_id">
            <thead>
                <tr>
                    <th scope="col">Id</th>
                    <th scope="col">User Id</th>
                    <th scope="col">Message</th>
                </tr>
            </thead>
            <tbody>
                {{#each feedbacks}}
                <tr>
                    <td >{{this._id}}</th>
                    <td>{{this.user._id}}</td>
                    <td>{{this.message}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</section>
<script>
    $(document).ready(function () {
        $('#table_id').DataTable();
    });
</script>